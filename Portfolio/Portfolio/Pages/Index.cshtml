@page
@using Portfolio.Models
@inject IProjectService ProjectService

@{ 
    Layout = "_Layout";
    var projects = await ProjectService.GetAllAsync();
}

<div class="container">
    <div class="row">
        @foreach(var proj in projects)
        {
            <div class="col-md-4">
                @proj.Name
            </div>
        }
    </div>
</div>
